<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
     <link rel="stylesheet" href="/css/style.css" />
        <title>Car Store</title>
    </head>
    <body>
  <%- include('./includes/nav.ejs', {isAuthorised:isAuthorised }) %>
        </div>
        <div class="cars">
            <% if(cars) {%> 
                <% cars.forEach((car) => { %>
            <div class="car">
                 <h1><%= car.model %></h1>
                <img src="<%= car.url%>" alt="<%= car.name %>" />
                <p>Price: $<%= car.price %></p>
            </div>
            <% }) %> <% } %>
        </div>
    </body>
</html>
